{
    "name": "sourecode/git",
    "type": "library",
    "description": "A simple wrapper for Git",
    "keywords": ["git"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Schilling",
            "email": "jason@sourecode.dev"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1",
        "symfony/process": "^5.3 || ^6.0",
        "symfony/filesystem": "^5.3 || ^6.0",
        "symfony/string": "^5.3 || ^6.0",
        "symfony/finder": "^5.3 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "SoureCode\\Component\\Git\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev"
}
